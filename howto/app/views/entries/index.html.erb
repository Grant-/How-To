<h1>All Entries</h1>



<ul>
	
		<% @entries.each do |entry| %>
	<li>
 
		<%= entry.title %>
	by  <%= entry.Author %>
		<%= entry.Description %>
		<%= entry.tags %>
	</li>
</ul>

<%= link_to 'Show Full Entry', entry %> |
<%= link_to 'Edit Entry', edit_entry_path(entry) %>

<% end %>



<%= link_to 'New Entry', new_entry_path %>

